Step 1: Create Tensorflow folder name tensorflow09 under home directory
virtualenv --system-site-packages ~/tensorflow09
Step 2: Activate
source ~/tensorflow09/bin/activate
Step 3:
export TF_BINARY_URL=https://storage.googleapis.com/tensorflow/linux/cpu/tensorflow-0.9.0-cp27-none-linux_x86_64.whl
pip install --ignore-installed --upgrade $TF_BINARY_URL
